{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "biglakeorchestrationDF"
		},
		"ADLSConnection_servicePrincipalKey": {
			"type": "secureString",
			"metadata": "Secure string for 'servicePrincipalKey' of 'ADLSConnection'"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"AzureHDInsightLinkedService_clusterPassword": {
			"type": "secureString",
			"metadata": "Secure string for 'clusterPassword' of 'AzureHDInsightLinkedService'"
		},
		"AzureHDInsightLinkedService_clusterSshPassword": {
			"type": "secureString",
			"metadata": "Secure string for 'clusterSshPassword' of 'AzureHDInsightLinkedService'"
		},
		"AzureHDInsightLinkedService_servicePrincipalKey": {
			"type": "secureString",
			"metadata": "Secure string for 'servicePrincipalKey' of 'AzureHDInsightLinkedService'"
		},
		"FileServerSystem_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'FileServerSystem'"
		},
		"StorageLinkedService_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'StorageLinkedService'"
		},
		"mssql_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'mssql'"
		},
		"ADLSConnection_properties_typeProperties_dataLakeStoreUri": {
			"type": "string",
			"defaultValue": "https://pocbiglake.azuredatalakestore.net/webhdfs/v1"
		},
		"ADLSConnection_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "2046243d-acda-4efe-8ed2-875dbc6e7d72"
		},
		"ADLSConnection_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "c8fedfb4-d804-457b-9745-362581de2e4c"
		},
		"ADLSConnection_properties_typeProperties_subscriptionId": {
			"type": "string",
			"defaultValue": "974668b8-821d-4cc2-a84d-c81a7733f464"
		},
		"ADLSConnection_properties_typeProperties_resourceGroupName": {
			"type": "string",
			"defaultValue": "poc"
		},
		"AzureHDInsightLinkedService_properties_typeProperties_hostSubscriptionId": {
			"type": "string",
			"defaultValue": "974668b8-821d-4cc2-a84d-c81a7733f464"
		},
		"AzureHDInsightLinkedService_properties_typeProperties_clusterResourceGroup": {
			"type": "string",
			"defaultValue": "poc"
		},
		"AzureHDInsightLinkedService_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "2046243d-acda-4efe-8ed2-875dbc6e7d72"
		},
		"AzureHDInsightLinkedService_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "c8fedfb4-d804-457b-9745-362581de2e4c"
		},
		"AzureHDInsightLinkedService_properties_typeProperties_clusterUserName": {
			"type": "string",
			"defaultValue": "admin"
		},
		"AzureHDInsightLinkedService_properties_typeProperties_clusterSshUserName": {
			"type": "string",
			"defaultValue": "sshadmin"
		},
		"FileServerSystem_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "ben"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas Token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADLSConnection_servicePrincipalKey": {
						"value": "[parameters('ADLSConnection_servicePrincipalKey')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"StorageLinkedService_connectionString": {
						"value": "[parameters('StorageLinkedService_connectionString')]"
					},
					"AzureHDInsightLinkedService_clusterPassword": {
						"value": "[parameters('AzureHDInsightLinkedService_clusterPassword')]"
					},
					"AzureHDInsightLinkedService_clusterSshPassword": {
						"value": "[parameters('AzureHDInsightLinkedService_clusterSshPassword')]"
					},
					"AzureHDInsightLinkedService_servicePrincipalKey": {
						"value": "[parameters('AzureHDInsightLinkedService_servicePrincipalKey')]"
					},
					"FileServerSystem_password": {
						"value": "[parameters('FileServerSystem_password')]"
					},
					"mssql_connectionString": {
						"value": "[parameters('mssql_connectionString')]"
					},
					"ADLSConnection_properties_typeProperties_dataLakeStoreUri": {
						"value": "[parameters('ADLSConnection_properties_typeProperties_dataLakeStoreUri')]"
					},
					"ADLSConnection_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('ADLSConnection_properties_typeProperties_servicePrincipalId')]"
					},
					"ADLSConnection_properties_typeProperties_tenant": {
						"value": "[parameters('ADLSConnection_properties_typeProperties_tenant')]"
					},
					"ADLSConnection_properties_typeProperties_subscriptionId": {
						"value": "[parameters('ADLSConnection_properties_typeProperties_subscriptionId')]"
					},
					"ADLSConnection_properties_typeProperties_resourceGroupName": {
						"value": "[parameters('ADLSConnection_properties_typeProperties_resourceGroupName')]"
					},
					"AzureHDInsightLinkedService_properties_typeProperties_hostSubscriptionId": {
						"value": "[parameters('AzureHDInsightLinkedService_properties_typeProperties_hostSubscriptionId')]"
					},
					"AzureHDInsightLinkedService_properties_typeProperties_clusterResourceGroup": {
						"value": "[parameters('AzureHDInsightLinkedService_properties_typeProperties_clusterResourceGroup')]"
					},
					"AzureHDInsightLinkedService_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('AzureHDInsightLinkedService_properties_typeProperties_servicePrincipalId')]"
					},
					"AzureHDInsightLinkedService_properties_typeProperties_tenant": {
						"value": "[parameters('AzureHDInsightLinkedService_properties_typeProperties_tenant')]"
					},
					"AzureHDInsightLinkedService_properties_typeProperties_clusterUserName": {
						"value": "[parameters('AzureHDInsightLinkedService_properties_typeProperties_clusterUserName')]"
					},
					"AzureHDInsightLinkedService_properties_typeProperties_clusterSshUserName": {
						"value": "[parameters('AzureHDInsightLinkedService_properties_typeProperties_clusterSshUserName')]"
					},
					"FileServerSystem_properties_typeProperties_userId": {
						"value": "[parameters('FileServerSystem_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}